{
  "name": "rubic-sdk",
  "version": "0.0.1",
  "description": "Simplify dApp creation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --profile --json > ./dist/stats.json && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "compile": "tsc --project tsconfig.json && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "test": "jest",
    "analyze": "webpack --profile --json > stats.json &&  webpack-bundle-analyzer stats.json"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "axios": "^0.22.0",
    "bignumber.js": "^9.0.1",
    "p-timeout": "^5.0.2",
    "rxjs": "^7.4.0",
    "ts-cacheable": "^1.0.5",
    "web3": "^1.5.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^27.4.5",
    "compression-webpack-plugin": "^9.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "^27.1.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.2.1",
    "ts-essentials": "^9.0.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.2",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cryptorubic/rubic-sdk.git"
  },
